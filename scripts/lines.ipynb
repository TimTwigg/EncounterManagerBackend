   "source": [
    "from pathlib import Path\n",
    "\n",
    "ROOT_DIR = Path(\"./..\")\n",
    "\n",
    "lines = 0\n",
    "\n",
    "for p in ROOT_DIR.glob(\"**/*.go\"):\n",
    "    if p.is_file():\n",
    "        with open(p, \"r\", encoding=\"utf-8\") as f:\n",
    "            lines += sum(1 for _ in f)\n",
    "\n",
    "print(f\"Total lines of Go code: {lines}\")"
   ]
  }
